        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.spillerForNavn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.spillerEfterNavn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.spillerOptagelsesDato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.spillerFunktion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.spillerUdmeldt)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.spillerPK }) |
                @Html.ActionLink("Details", "Details", new { id = item.spillerPK }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.spillerPK }) |
                @Html.ActionLink("Games", "Index", "Spil", new { spillerID = item.spillerPK }, item.spillerPK)
