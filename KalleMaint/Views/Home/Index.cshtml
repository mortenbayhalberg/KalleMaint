@model IEnumerable<KalleMaint.Models.tblSpiller>

@{
    ViewBag.Title = "Index spiller";
}

<h2>Index profiles</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.spillerForNavn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.spillerEfterNavn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.spillerOptagelsesDato)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.spillerFunktion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.spillerUdmeldt)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
